   "source": [
    "rot13 = {chr(ord('a')+i): chr(ord('a')+(i+13) % 26) for i in range(26)}\n",
    "\n",
    "s = 'klartext'\n",
    "encoded = [\n",
    "    rot13[char.lower()].upper() if char.isupper() else rot13[char]\n",
    "    if char.lower() in rot13\n",
    "    else char\n",
    "    for char in s\n",
    "]"
   ]
